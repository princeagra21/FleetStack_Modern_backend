# Environment Configuration for FleetStack Backend

# Server Configuration
NODE_ENV=development
PORT=3000

# Primary Database Configuration (FleetStack_db)
PRIMARY_DB_HOST=localhost
PRIMARY_DB_USER=postgres
PRIMARY_DB_PORT=5432
PRIMARY_DB_PASSWORD=password
PRIMARY_DB_NAME=FleetStack_db
PRIMARY_DATABASE_URL="postgresql://postgres:password@localhost:5432/FleetStack_db?schema=public"

# Logs Database Configuration (FleetStack_logs)
LOGS_DB_HOST=localhost
LOGS_DB_USER=postgres
LOGS_DB_PORT=5433
LOGS_DB_PASSWORD=password
LOGS_DB_NAME=FleetStack_logs
LOGS_DATABASE_URL="postgresql://postgres:password@localhost:5433/FleetStack_logs?schema=public"

# Address Database Configuration (FleetStack_Address)
ADDRESS_DB_HOST=localhost
ADDRESS_DB_USER=postgres
ADDRESS_DB_PORT=5434
ADDRESS_DB_PASSWORD=password
ADDRESS_DB_NAME=FleetStack_Address
ADDRESS_DATABASE_URL="postgresql://postgres:password@localhost:5434/FleetStack_Address?schema=public"

# JWT Configuration (Optional for future use)
JWT_SECRET=your-super-secret-jwt-key
JWT_EXPIRATION_TIME=3600

# Redis Configuration
REDIS_HOST=127.0.0.1
REDIS_PORT=6379
REDIS_PASSWORD=your-redis-password
REDIS_TLS=false
# Alternative: Use REDIS_URL for full connection string
# REDIS_URL=redis://username:password@hostname:port/database

# Logging Configuration
LOG_ENABLE=true
LOGGER_LEVEL=INFO
MAX_RESP_LOG_STRING=300

