{
  "name": "fleetstack-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:cluster": "pm2 start ecosystem.config.js --env production",
    "start:cluster:dev": "pm2 start ecosystem.config.js --env development",
    "start:optimized": "npm run start:dev",
    "cluster:stop": "pm2 stop ecosystem.config.js",
    "cluster:restart": "pm2 restart ecosystem.config.js",
    "cluster:status": "pm2 status",
    "cluster:logs": "pm2 logs",
    "cluster:monit": "pm2 monit",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prisma:generate:primary": "prisma generate --schema=src/database/prisma/primary.prisma",
    "prisma:generate:logs": "prisma generate --schema=src/database/prisma/logs.prisma",
    "prisma:generate:address": "prisma generate --schema=src/database/prisma/address.prisma",
    "prisma:generate:all": "npm run prisma:generate:primary && npm run prisma:generate:logs && npm run prisma:generate:address",
    "prisma:ensure-clients": "node scripts/ensure-prisma-clients.js",
    "prisma:migrate:primary": "prisma migrate dev --schema=src/database/prisma/primary.prisma --name",
    "prisma:migrate:logs": "prisma migrate dev --schema=src/database/prisma/logs.prisma --name",
    "prisma:migrate:address": "prisma migrate dev --schema=src/database/prisma/address.prisma --name",
    "prisma:deploy:primary": "prisma migrate deploy --schema=src/database/prisma/primary.prisma",
    "prisma:deploy:logs": "prisma migrate deploy --schema=src/database/prisma/logs.prisma",
    "prisma:deploy:address": "prisma migrate deploy --schema=src/database/prisma/address.prisma",
    "prisma:deploy:all": "npm run prisma:deploy:primary && npm run prisma:deploy:logs && npm run prisma:deploy:address",
    "prisma:reset:primary": "prisma migrate reset --schema=src/database/prisma/primary.prisma --force",
    "prisma:reset:logs": "prisma migrate reset --schema=src/database/prisma/logs.prisma --force",
    "prisma:reset:address": "prisma migrate reset --schema=src/database/prisma/address.prisma --force",
    "prisma:studio:primary": "prisma studio --schema=src/database/prisma/primary.prisma",
    "prisma:studio:logs": "prisma studio --schema=src/database/prisma/logs.prisma",
    "prisma:studio:address": "prisma studio --schema=src/database/prisma/address.prisma",
    "postinstall": "npm run prisma:generate:all"
  },
  "dependencies": {
    "@bull-board/api": "^6.12.7",
    "@bull-board/fastify": "^6.12.7",
    "@bull-board/nestjs": "^6.12.7",
    "@fastify/compress": "^8.0.1",
    "@fastify/cors": "^11.0.0",
    "@fastify/helmet": "^13.0.0",
    "@fastify/rate-limit": "^10.1.1",
    "@fastify/under-pressure": "^9.0.0",
    "@nest-lab/throttler-storage-redis": "^1.1.0",
    "@nestjs/axios": "^4.0.1",
    "@nestjs/bullmq": "^11.0.3",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-fastify": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^6.16.0",
    "@types/bcrypt": "^6.0.0",
    "@types/node-cron": "^3.0.11",
    "@types/passport-jwt": "^4.0.1",
    "@types/pg": "^8.15.5",
    "@types/winston": "^2.4.4",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "bullmq": "^5.58.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^17.2.2",
    "ioredis": "^5.7.0",
    "nestjs-i18n": "^10.5.1",
    "node-cron": "^4.2.1",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "prisma": "^6.16.0",
    "public-ip": "^7.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.1.6",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.18.1",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
